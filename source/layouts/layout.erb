<!doctype html>
<html lang="pt-br">
<head>
  <meta charset="utf-8">
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
  <title><%= data.page.title %></title>
  <!--[if IE]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <%#= partial "meta-social" %>
  <%= favicon_tag 'favicon.ico' %>
   <%= stylesheet_link_tag current_page.url.to_s.gsub! '/', '' %>
   <%= stylesheet_link_tag  'espn/footer.min','espn/header.min' %>
  <%= javascript_include_tag  'espn/application-head' %>
  <script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-39636870-1']);
    _gaq.push(['_trackPageview']);
    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
  </script>
</head>
<body class="<%= page_classes %>">
  <%= partial "header" %>
  <main role="main">
    <%= yield %>
  </main>
  <%= partial "footer" %>
  <%= javascript_include_tag  current_page.url.to_s.gsub! '/', '' %>
  <%= javascript_include_tag  'espn/header', 'espn/application' %>
</body>
</html>