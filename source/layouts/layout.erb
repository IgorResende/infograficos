<!doctype html>
<html lang="pt-br">
<head>
  <meta charset="utf-8">
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
  <title><%= data.page.title %></title>
  <!--[if IE]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <%#= partial "meta-social" %>
  <%= favicon_tag 'favicon.ico' %>
   <%= stylesheet_link_tag current_page.url.to_s.gsub! '/', '' %>
   <%= stylesheet_link_tag  'espn/footer.min','espn/header.min' %>
  <%= javascript_include_tag  'espn/application-head' %>
</head>
<body class="<%= page_classes %>">
  <%= partial "header" %>
  <%= yield %>
  <%= partial "footer" %>
  <%= javascript_include_tag  current_page.url.to_s.gsub! '/', '' %>
  <%= javascript_include_tag  'espn/header' %>
</body>
</html>