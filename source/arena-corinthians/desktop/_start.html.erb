<div id="start" class="section">
  <div class="container">
    <div class="title">
      <h1>
        <%= data.info.start.title %>
      </h1>
      <p>
        <%= data.info.start.desc %>
      </p>
    </div>
    <!-- <div id="arena-corinthians"></div> -->
    <ul id="pontos">
      <li id="ponto-1"></li>
      <li id="ponto-2"></li>
      <li id="ponto-3"></li>
      <li id="ponto-4"></li>
      <li id="ponto-5"></li>
      <li id="ponto-6"></li>
      <li id="ponto-7"></li>
      <li id="ponto-8"></li>
      <li id="ponto-9"></li>
    </ul>
    <div id="menu">
      <ul>
      <% data.info.start.menu.each do |menu| %>
        <li>
          <a href="#">
            <%= menu.title %>
            <span>
              <%= menu.desc %>
            </span>
          </a>
        </li>
      <% end %>
      </ul>
    </div>
  </div>
</div>